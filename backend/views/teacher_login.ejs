        <div class="container" style="background: url(/img/1.jpg);background-repeat: no-repeat;background-size: cover;border-radius: 50px">
            <div class="jumbotron jumbotron-fluid">
              <div class="container">
                <h1 class="display-4" style="text-align:center; width:100%;">PROFESSOR LOGIN</h1>
                <div class="row">
                    <div class="col-sm-2"></div>
                    <div id="login_input"class="col-sm-6" style="margin:auto;">
                        <form action="/TLogin" method="post" style="margin:20px;" class="validated-form" novalidate>
                          <div class="form-group">
                            <label for="exampleInputSid">PROFESSOR EMAIL</label>
                            <input type="email" class="form-control" id="exampleInputSid" placeholder="Email" name="Email" required>
                          </div>
                          <div class="form-group">
                            <label for="exampleInputPassword1">PASSWORD</label>
                            <input type="password" class="form-control" id="exampleInputPassword1" placeholder="********" name="Password" required>
                          </div>
                          <button type="submit" class="btn">Submit</button>
                        </form>
                    </div>
                    <div class="col-sm-2"></div>
                </div>
                <div class="row">
                    <div id="new_account" class="col-sm-6" style="margin:auto;">
                        <div class="card text-center">
                          <div class="card-body">
                            <a href="/TSignup" class="btn">NEW ACCOUNT</a>
                          </div>
                        </div>
                    </div>
                </div>
              </div>
